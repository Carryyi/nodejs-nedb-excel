{{#section 'css'}}
    <link rel="stylesheet" href="fullcalendar/bootstrap-fullcalendar.css"/>
    <link rel="stylesheet" href="fileinput/fileinput.min.css"/>
{{/section}}
{{#section 'js'}}
    <script src="fullcalendar/fullcalendar.min.js"></script>
    <script src="fileinput/fileinput.min.js"></script>
    <script src="fileinput/fileinput_zh.js"></script>
    <script src="javascripts/lib/external-dragging-calendar.js"></script>
    <script src="javascripts/cal.js"></script>
    <script id="ul_daduis_template" type="text/x-handlebars-template">
        \{{#if daduis}}
        \{{#each daduis}}
        \{{#if @first}}
        <li class="active" ><a data-toggle="tab" onclick="tab_dataInit(this)"  href="#id_dadui_\{{id}}">\{{name}}</a></li>
        \{{else}}
        <li><a data-toggle="tab" onclick="tab_dataInit(this)" href="#id_dadui_\{{id}}">\{{name}}</a></li>
        \{{/if}}

        \{{/each}}
        \{{else}}
        <i class="fa fa-info-circle">无数据</i>
        \{{/if}}
    </script>
    <script id="tab_zhongdui_template" type="text/x-handlebars-template">
        \{{#each daduis}}
        \{{#if @first}}
        <div class="tab-pane active" id="id_dadui_\{{id}}"></div>
        \{{else}}
        <div class="tab-pane" id="id_dadui_\{{id}}"></div>
        \{{/if}}
        \{{/each}}
    </script>
    <script id="block_zhongdui_template" type="text/x-handlebars-template">
        \{{#each zhongduis}}
        <div class="row">
            <div class="col-lg-12">
                <section class="panel">
                    <header class="panel-heading">
                        <b>\{{zhongdui_name}}</b>
                    </header>
                    <div class="panel-body">
                        \{{#each data}}
                        <section class="panel   col-lg-12">
                            <div class="post-bg">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-xs-9">
                                            \{{name}} \{{star type}}
                                        </div>
                                        <div class="col-xs-3">
                                            <i class="fa fa-volume-control-phone" title="呼号">&nbsp;\{{callnumber}}</i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="profile-nav alt green-border">
                                <ul class="nav nav-pills nav-stacked">
                                    \{{work worktime}}
                                    <li title="高峰点"><a> <i class="fa fa-industry"></i>&nbsp;\{{peakpoint}} <i class="fa fa-edit" style="float:right;cursor:pointer"></i></a></li>
                                    <li title="高峰时间"><a> <i class="fa fa-warning"></i>&nbsp;\{{peaktime}}<i class="fa fa-edit" style="float:right;cursor:pointer"></i></a></li>
                                    <li title="工歇时间"><a> <i class="fa fa-coffee"></i>工歇&nbsp;\{{resttime}}<i class="fa fa-edit" style="float:right;cursor:pointer"></i></a></li>
                                    <li title="就餐时间"><a> <i class="fa fa-cutlery"></i>&nbsp;就餐&nbsp;\{{mealtime}}<i class="fa fa-edit" style="float:right;cursor:pointer"></i></a></li>
                                    <li title="责任区"><a> <i class="fa fa-area-chart"></i>&nbsp;\{{responsiblearea}}<i class="fa fa-edit" style="float:right;cursor:pointer"></i></a></li>
                                    <li title="巡线"><a> <i class="fa fa-line-chart"></i>&nbsp;\{{patrolline}}<i class="fa fa-edit" style="float:right;cursor:pointer"></i></a></li>
                                    <li title="值班人员"><a> <i class="fa fa-user"></i>&nbsp;张羽<i class="fa fa-edit" style="float:right;cursor:pointer"></i></a></li>
                                </ul>
                            </div>

                        </section>
                        \{{/each}}
                    </div>
                </section>
            </div>
        </div>
        \{{/each}}
    </script>
{{/section}}